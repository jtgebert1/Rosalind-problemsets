#!/usr/bin/env python3
DNAseq=''

#convert string to list and create new variable 
DNAseq_list=list(DNAseq)

#count
def count(lst, x):
	count=0
	for ele in lst:
		if (ele == x):
			count = count + 1
	return count

#count A
lst=DNAseq_list
x='A'
Acount=count(lst, x)

#count C
lst=DNAseq_list
x='C'
Ccount=count(lst, x)

#count G
lst=DNAseq_list
x='G'
Gcount=count(lst, x)

#count T
lst=DNAseq_list
x='T'
Tcount=count(lst, x)


print(int(Acount), int(Ccount), int(Gcount), int(Tcount))
